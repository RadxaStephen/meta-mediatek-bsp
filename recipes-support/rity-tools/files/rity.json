{
  "rity-tools": {
    "min-version": "1.0.0.post0"
  },
  "partitions": {
    "mmc0": null,
    "mmc0boot0": "bl2.img",
    "mmc0boot1": "u-boot-env.bin",
    "bootloaders": "fip.bin",
    "kernel": "fitImage",
    "rootfs": null
  },
  "groups": {
    "all": {
      "erase": ["mmc0", "mmc0boot0", "mmc0boot1"],
      "flash": ["mmc0", "mmc0boot0", "mmc0boot1"]
    },

    "system": {
      "flash": ["kernel", "rootfs"]
    }
  }
}
