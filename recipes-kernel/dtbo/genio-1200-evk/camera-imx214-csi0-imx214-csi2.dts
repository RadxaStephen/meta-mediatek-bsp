/dts-v1/;
/plugin/;

#include "camera-common.dtsi"

/ {
	fragment@10 {
		target = <&i2c0>;
		__overlay__ {
			sensor0 {
				sensor-names = "imx214_mipi_raw";
				reg = <0x1a>;
				status = "okay";
			};
			sensor1 {
				status = "disabled";
			};
		};
	};

	fragment@11 {
		target = <&i2c1>;
		__overlay__ {
			sensor2 {
				sensor-names = "imx214_mipi_raw";
				reg = <0x1a>;
				status = "okay";
			};
		};
	};

	fragment@12 {
		target = <&mtkcam0>;
		__overlay__ {
			status = "okay";
		};
	};

	fragment@13 {
		target = <&mtkcam1>;
		__overlay__ {
			status = "okay";
		};
	};
};